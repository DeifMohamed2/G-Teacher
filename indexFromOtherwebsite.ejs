        breakdownItems.splice(1, 0, {
          label: `${exam2Name}2 Score:`,
          value: calculationData.breakdown.exam2Score.toFixed(2)
        });
      }

      // Calculate proper spacing for items
      const itemSpacing = 55;
      const startY = breakdownY + 55;

      breakdownItems.forEach((item, index) => {
        const itemY = startY + (index * itemSpacing);

        // Draw separator line (except for first item)
        if (index > 0) {
          ctx.strokeStyle = 'rgba(30, 41, 59, 0.12)';
          ctx.lineWidth = 1;
          ctx.beginPath();
          ctx.moveTo(200, itemY - 30);
          ctx.lineTo(canvas.width - 200, itemY - 30);
          ctx.stroke();
        }

        // Enhanced label styling with better spacing
        ctx.fillStyle = '#1e293b';
        ctx.font = 'bold 18px Arial';
        ctx.textAlign = 'left';
        ctx.textBaseline = 'middle';
        ctx.fillText(item.label, 200, itemY);

        // Enhanced value styling with proper alignment
        ctx.fillStyle = '#1e293b';
        ctx.font = 'bold 18px Arial';
        ctx.textAlign = 'right';
        ctx.textBaseline = 'middle';
        ctx.fillText(item.value, canvas.width - 200, itemY);
      });

      // Enhanced decorative elements
      ctx.strokeStyle = '#B80101';
      ctx.lineWidth = 4;
      ctx.lineCap = 'round';

      // Top decorative line with gradient effect
      const topLineGradient = ctx.createLinearGradient(50, 150, canvas.width - 50, 150);
      topLineGradient.addColorStop(0, 'rgba(184, 1, 1, 0.3)');
      topLineGradient.addColorStop(0.5, '#B80101');
      topLineGradient.addColorStop(1, 'rgba(184, 1, 1, 0.3)');
      ctx.strokeStyle = topLineGradient;
      
      ctx.beginPath();
      ctx.moveTo(50, 150);
      ctx.lineTo(canvas.width - 50, 150);
      ctx.stroke();

      // Bottom decorative line
      const bottomLineGradient = ctx.createLinearGradient(50, canvas.height - 30, canvas.width - 50, canvas.height - 30);
      bottomLineGradient.addColorStop(0, 'rgba(184, 1, 1, 0.3)');
      bottomLineGradient.addColorStop(0.5, '#B80101');
      bottomLineGradient.addColorStop(1, 'rgba(184, 1, 1, 0.3)');
      ctx.strokeStyle = bottomLineGradient;
      
      ctx.beginPath();
      ctx.moveTo(50, canvas.height - 30);
      ctx.lineTo(canvas.width - 50, canvas.height - 30);
      ctx.stroke();
      
      // Reset stroke style
      ctx.strokeStyle = '#B80101';

      // Enhanced footer with Elkably branding
      ctx.fillStyle = '#94a3b8';
      ctx.font = 'italic 15px Arial';
      ctx.textAlign = 'center';
      ctx.fillText('Generated by Elkably', canvas.width / 2, canvas.height - 12);

      // Convert canvas to image and share
      canvas.toBlob((blob) => {
        if (navigator.share && navigator.canShare && navigator.canShare({
            files: [new File([blob], 'score-result.png', {
              type: 'image/png'
            })]
          })) {
          const file = new File([blob], 'my-university-score-elkably.png', {
            type: 'image/png'
          });
          navigator.share({
            title: 'My University Admission Score - Elkably',
            text: `Check out my university admission score calculated with Elkably!`,
            files: [file]
          });
        } else {
          // Fallback: download image
          const link = document.createElement('a');
          link.download = 'my-university-score-elkably.png';
          link.href = canvas.toDataURL();
          link.click();
        }
      }, 'image/png');
    }

    // Reset calculator
    function resetCalculator() {
      // Reset selections
      universityOptions.forEach(option => option.classList.remove('selected'));
      selectedUniversityType = null;
      
      examOptions.forEach(option => {
        option.classList.remove('selected');
      });
      selectedExamType = null;
      selectedExam2Type = null;

      // Clear inputs
      if (exam1Input) {
        exam1Input.value = '';
        exam1Input.classList.remove('error');
      }
      if (exam2Input) {
        exam2Input.value = '';
        exam2Input.classList.remove('error');
      }
      if (exam2TypeSelect) {
        exam2TypeSelect.value = '';
      }
      if (exam2ScoreGroup) {
        exam2ScoreGroup.style.display = 'none';
      }
      if (gpaInput) {
        gpaInput.value = '';
        gpaInput.classList.remove('error');
      }

      // Reset buttons
      if (nextStep1Btn) nextStep1Btn.disabled = true;
      if (nextStep2Btn) nextStep2Btn.disabled = true;
      if (nextStep3Btn) nextStep3Btn.disabled = true;

      // Reset calculation data
      calculationData = {
        exam1: 0,
        exam2: 0,
        gpa: 0,
        finalScore: 0,
        breakdown: {
          exam1Score: 0,
          exam2Score: 0,
          gpaScore: 0
        }
      };

      // Reset show more button
      const showMoreBtn = document.getElementById('showMoreBtn');
      if (showMoreBtn) {
        showMoreBtn.style.display = 'none';
        showMoreBtn.classList.remove('expanded');
        const showMoreText = showMoreBtn.querySelector('.show-more-text');
        if (showMoreText) {
          showMoreText.textContent = 'Show More';
        }
      }

      // Reset calculated value
      if (calculatedValue) {
        calculatedValue.textContent = '0.00';
        calculatedValue.removeAttribute('data-full-score');
        calculatedValue.removeAttribute('data-is-expanded');
      }

      // Hide available colleges card
      const availableCollegesCard = document.getElementById('availableCollegesCard');
      if (availableCollegesCard) {
        availableCollegesCard.style.display = 'none';
      }

      // Clear search input
      const collegesSearch = document.getElementById('collegesSearch');
      if (collegesSearch) {
        collegesSearch.value = '';
      }
    }

    // Initialize calculator
    initCalculator();
  });
</script>


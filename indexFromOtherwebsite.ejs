  /* Results Score Container */
  .results-score {
    padding: 20px 15px;
    max-width: 100%;
    overflow-x: auto;
    overflow-y: hidden;
    -webkit-overflow-scrolling: touch;
  }

  /* Hide scrollbar but keep functionality */

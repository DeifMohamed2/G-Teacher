    // Setup step navigation
    function setupStepNavigation() {
      // Step item click handlers
      stepItems.forEach((item, index) => {
        item.addEventListener('click', () => {
          const stepNumber = parseInt(item.dataset.step);
          
          // Check if step is disabled
          if (item.classList.contains('disabled')) {
            return; // Don't allow navigation to disabled steps
          }
          
          // Allow navigation if step is already completed, current step, or next step
          const canNavigate = 
            stepNumber === 1 || 
            (stepNumber === 2 && selectedUniversityType) ||
            (stepNumber === 3 && selectedUniversityType && selectedExamType) ||
            (stepNumber === 4 && selectedUniversityType && selectedExamType && 
             parseFloat(exam1Input?.value) > 0 && parseFloat(gpaInput?.value) > 0);
          
          if (canNavigate) {
            showStep(stepNumber);
          }
        });
      });
    }

    // Setup university selection
    function setupUniversitySelection() {
      universityOptions.forEach(option => {
        option.addEventListener('click', () => {
          // Remove previous selection
          universityOptions.forEach(opt => opt.classList.remove('selected'));

          // Add selection to clicked option
          option.classList.add('selected');
          selectedUniversityType = option.dataset.type;

          // Enable next button
          nextStep1Btn.disabled = false;

          // Update minimum requirements
          updateMinimumRequirements();
          
          // Update step indicators
          updateStepIndicators();
        });
      });
    }


<!-- DotLottie Web Component Script -->
<script src="https://unpkg.com/@lottiefiles/dotlottie-wc@0.8.11/dist/dotlottie-wc.js" type="module"></script>

<script>
  // Welcome Overlay Controller
  (function() {
    'use strict';
    
    // Check if user has seen the welcome overlay before
    const hasSeenWelcome = localStorage.getItem('elkablyWelcomeSeen');
    const welcomeOverlay = document.getElementById('welcomeOverlay');
    
    if (!hasSeenWelcome && welcomeOverlay) {
      const startBtn = document.getElementById('startJourneyBtn');
      const countdownDisplay = document.getElementById('countdownDisplay');
      const countdownText = document.querySelector('.countdown-text');
      const rocketLaunch = document.getElementById('rocketLaunch');
      const welcomeContent = document.querySelector('.welcome-content');
      const welcomeTitle = document.querySelector('.welcome-title');
      const welcomeSubtitle = document.querySelector('.welcome-subtitle');
      const logoContainer = document.querySelector('.welcome-logo-container');
      
      // Show the overlay
      setTimeout(() => {
        welcomeOverlay.classList.add('active');
      }, 100);
      
      // Prevent body scroll while overlay is active
      document.body.style.overflow = 'hidden';
      
      // Handle start button click
      if (startBtn && countdownDisplay && countdownText && rocketLaunch) {
        startBtn.addEventListener('click', function(e) {
          e.preventDefault();
          
          // Hide button completely
          startBtn.classList.add('hidden');
          
          // Hide logo container with smooth professional fade-out
          if (logoContainer) {
            logoContainer.style.transition = 'opacity 1s cubic-bezier(0.4, 0, 0.2, 1), transform 1s cubic-bezier(0.4, 0, 0.2, 1)';
            logoContainer.style.opacity = '0';
            logoContainer.style.transform = 'scale(0.8) translateY(-20px)';
            setTimeout(() => {
              logoContainer.classList.add('hidden');
            }, 1000);
          }
          
          // Hide welcome title with smooth professional fade-out
          if (welcomeTitle) {
            welcomeTitle.style.transition = 'opacity 1s cubic-bezier(0.4, 0, 0.2, 1), transform 1s cubic-bezier(0.4, 0, 0.2, 1)';
            welcomeTitle.style.opacity = '0';
            welcomeTitle.style.transform = 'translateY(-30px) scale(0.95)';
          }
          if (welcomeSubtitle) {
            welcomeSubtitle.style.transition = 'opacity 1s cubic-bezier(0.4, 0, 0.2, 1), transform 1s cubic-bezier(0.4, 0, 0.2, 1)';
            welcomeSubtitle.style.opacity = '0';
            welcomeSubtitle.style.transform = 'translateY(-20px)';
          }
          
          // Wait for fade, then show countdown and rocket in preparation
          setTimeout(() => {
            if (welcomeTitle) welcomeTitle.style.display = 'none';
            if (welcomeSubtitle) welcomeSubtitle.style.display = 'none';
            
            // Show countdown display
            countdownDisplay.classList.add('show');
            
            // Show rocket in preparation mode (ready to launch) with smooth entrance
            rocketLaunch.classList.add('preparation');
            
            // Countdown sequence: 5, 4, 3, 2, 1, Goooo!
            const countdownSequence = ['5', '4', '3', '2', '1', 'Goo!'];
            let index = 0;
            
            function displayNextNumber() {
              if (index < countdownSequence.length) {
                // Update text
                countdownText.textContent = countdownSequence[index];
                
                // Remove animation class
                countdownText.classList.remove('animate');
                
                // Force reflow to restart animation
                void countdownText.offsetWidth;
                
                // Add animation class
                countdownText.classList.add('animate');
                
                index++;
                
                // Continue countdown or launch rocket
                if (index < countdownSequence.length) {
                  setTimeout(displayNextNumber, 900);
                } else {
                  // When "Goo!" appears, launch the rocket
                  setTimeout(() => {
                    // Hide countdown
                    countdownDisplay.classList.remove('show');
                    
                    // Small delay to let "Goo!" be visible, then launch
                    setTimeout(() => {
                      // Remove preparation class and add launch class to start rocket animation
                      rocketLaunch.classList.remove('preparation');
                      rocketLaunch.classList.add('launch');
                      
                      console.log('Rocket launch animation started');
                      
                      // Hide overlay right after rocket animation completes (2.5s animation)
                      setTimeout(() => {
                        welcomeOverlay.classList.add('fade-out');
                        
                        // Clean up and mark as seen
                        setTimeout(() => {
                          welcomeOverlay.remove();
                          localStorage.setItem('elkablyWelcomeSeen', 'true');
                          document.body.style.overflow = '';
                        }, 800);
                      }, 1200);
                    }, 500);
                  }, 700);
                }
              }
            }
            
            // Start the countdown
            displayNextNumber();
          }, 1100);
        });
      }
    } else if (welcomeOverlay) {
      // User has seen it before, remove immediately
      welcomeOverlay.remove();
    }
  })();
</script>


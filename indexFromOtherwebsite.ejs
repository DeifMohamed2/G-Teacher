  /* Exam Options Styling - Matching University Options Design */
  .exam-options {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 20px;
    margin-bottom: 30px;
  }

  .exam-option {
    background: rgba(255, 255, 255, 0.6);
    border: 2px solid rgba(30, 41, 59, 0.1);
    border-radius: 15px;
    padding: 20px;
    cursor: pointer;
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
    display: flex;
    align-items: center;
    gap: 15px;
  }

  .exam-option::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(184, 1, 1, 0.05), transparent);
    transition: left 0.5s ease;
  }

  .exam-option:hover {
    border-color: #B80101;
    background: rgba(184, 1, 1, 0.05);
    transform: translateY(-3px);
    box-shadow: 0 10px 25px rgba(184, 1, 1, 0.15);
  }

  .exam-option:hover::before {
    left: 100%;
  }

  .exam-option.selected {
    border-color: #B80101;
    background: rgba(184, 1, 1, 0.1);
    box-shadow: 0 10px 25px rgba(184, 1, 1, 0.2);
  }

  /* Exam option icon with image */
  .exam-option .option-icon {
    width: 50px;
    height: 50px;
    background: rgba(255, 255, 255, 0.9);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
    padding: 5px;
    border: 1px solid rgba(30, 41, 59, 0.1);
  }

  .exam-option .option-icon .exam-logo-img {
    width: 100%;
    height: 100%;
    object-fit: contain;
    border-radius: 6px;
  }

  /* Exam option content */
  .exam-option .option-content {
    flex: 1;
  }

  .exam-option .option-content h4 {
    color: #1e293b;
    font-size: 1.1rem;
    font-weight: 600;
    margin-bottom: 5px;
  }

  .exam-option .option-content p {
    color: rgba(30, 41, 59, 0.7);
    font-size: 0.9rem;
    margin-bottom: 0;
  }

  /* Exam option check (radio button style) */
  .exam-option .option-check {
    width: 25px;
    height: 25px;
    border: 2px solid rgba(30, 41, 59, 0.3);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all 0.3s ease;
    flex-shrink: 0;
    background: transparent;
  }

  .exam-option.selected .option-check {
    background: #B80101;
    border-color: #B80101;
  }

  .exam-option .option-check i {
    color: white;
    font-size: 0.7rem;
    opacity: 0;
    transition: opacity 0.3s ease;
  }

  .exam-option.selected .option-check i {
    opacity: 1;
  }


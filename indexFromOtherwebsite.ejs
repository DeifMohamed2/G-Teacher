    // Show specific step
    function showStep(stepNumber) {
      // Validate step accessibility
      const canAccess = 
        stepNumber === 1 || 
        (stepNumber === 2 && selectedUniversityType) ||
        (stepNumber === 3 && selectedUniversityType && selectedExamType) ||
        (stepNumber === 4 && selectedUniversityType && selectedExamType && 
         parseFloat(exam1Input?.value) > 0 && parseFloat(gpaInput?.value) > 0);
      
      if (!canAccess) {
        return; // Don't allow navigation to inaccessible steps
      }

      // Hide all steps
      calculatorSteps.forEach(step => step.classList.remove('active'));

      // Show target step
      const targetStep = document.getElementById(`step${stepNumber}`);
      if (targetStep) {
        targetStep.classList.add('active');
      }

      // Update current step
      currentStep = stepNumber;

      // Update progress
      updateProgress();

      // Update step indicators
      updateStepIndicators();

      // If showing step 4, calculate and display results
      if (stepNumber === 4) {
        calculateAndDisplayResults();
      }
      
      // Update labels when moving to step 3
      if (stepNumber === 3) {
        updateScoreInputLabels();
      }
    }

    // Update progress bar
    function updateProgress() {
      const progressPercentage = ((currentStep - 1) / 3) * 100;
      if (progressFill) {
        progressFill.style.width = `${progressPercentage}%`;
      }
    }

    // Update step indicators
    function updateStepIndicators() {
      stepItems.forEach((item, index) => {
        const stepNumber = index + 1;
        item.classList.remove('active', 'completed', 'disabled');

        // Determine if step is accessible
        const isAccessible = 
          stepNumber === 1 || 
          (stepNumber === 2 && selectedUniversityType) ||
          (stepNumber === 3 && selectedUniversityType && selectedExamType) ||
          (stepNumber === 4 && selectedUniversityType && selectedExamType && 
           parseFloat(exam1Input?.value) > 0 && parseFloat(gpaInput?.value) > 0);

        if (stepNumber === currentStep) {
          item.classList.add('active');
        } else if (stepNumber < currentStep) {
          item.classList.add('completed');
        } else if (!isAccessible) {
          item.classList.add('disabled');
        }
      });
    }

    // Validate inputs
    function validateInputs() {
      if (!selectedUniversityType || !selectedExamType) return false;

      const exam1 = parseFloat(exam1Input?.value) || 0;
      const exam2 = parseFloat(exam2Input?.value) || 0;
      const gpa = parseFloat(gpaInput?.value) || 0;

      let hasErrors = false;

      // Remove previous error states
      if (exam1Input) exam1Input.classList.remove('error');
      if (exam2Input) exam2Input.classList.remove('error');
      if (gpaInput) gpaInput.classList.remove('error');

      // Validate ranges based on exam type
      // Exam 1 validation
      const exam1Max = selectedExamType === 'ACT' ? 36 : 1600;
      if (exam1 < 0 || exam1 > exam1Max) {
        if (exam1Input) exam1Input.classList.add('error');
        hasErrors = true;
      }
      
      // Exam 2 validation
      if (selectedExam2Type && selectedExam2Type !== 'NONE') {
        const exam2Max = selectedExam2Type === 'ACT' ? 36 : 1600;
        if (exam2 < 0 || exam2 > exam2Max) {
          if (exam2Input) exam2Input.classList.add('error');
          hasErrors = true;
        }
      }
      
      if (gpa < 0 || gpa > 4) {
        if (gpaInput) gpaInput.classList.add('error');
        hasErrors = true;
      }

      return !hasErrors;
    }


  /* Compact Professional Progress Line Box */
  .calculator-progress {
    background: rgba(255, 255, 255, 0.95);
    backdrop-filter: blur(10px);
    border-radius: 12px;
    padding: 15px 20px;
    margin: 20px auto 30px;
    max-width: 800px;
    box-shadow: 
      0 4px 15px rgba(0, 0, 0, 0.08),
      0 0 0 1px rgba(184, 1, 1, 0.1) inset;
    border: 1px solid rgba(184, 1, 1, 0.15);
    position: relative;
  }

  .progress-steps {
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
    gap: 10px;
  }

  .progress-line {
    position: absolute;
    top: 32px;
    left: 50px;
    right: 50px;
    height: 3px;
    background: rgba(30, 41, 59, 0.1);
    z-index: 1;
    border-radius: 2px;
  }

  .progress-fill {
    height: 100%;
    background: linear-gradient(90deg, #B80101 0%, #8B0000 100%);
    border-radius: 2px;
    transition: width 0.5s cubic-bezier(0.4, 0, 0.2, 1);
    width: 0%;
    box-shadow: 0 2px 4px rgba(184, 1, 1, 0.3);
  }

  .step-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 8px;
    cursor: pointer;
    transition: all 0.3s ease;
    flex: 1;
    position: relative;
    z-index: 2;
    padding: 5px;
  }

  .step-item.disabled {
    cursor: not-allowed;
    opacity: 0.4;
    pointer-events: none;
  }

  .step-number {
    width: 32px;
    height: 32px;
    border-radius: 50%;
    background: rgba(30, 41, 59, 0.08);
    border: 2px solid rgba(30, 41, 59, 0.2);
    display: flex;

    // Smart decimal display with toggle functionality
    function displayScoreWithToggle(score, element) {
      const showMoreBtn = document.getElementById('showMoreBtn');
      const fullScore = score.toString();
      
      // Check if score has more than 3 decimal places
      const decimalPart = fullScore.split('.')[1] || '';
      
      if (decimalPart.length > 3) {
        // Store full score in data attribute
        element.setAttribute('data-full-score', fullScore);
        element.setAttribute('data-is-expanded', 'false');
        
        // Show only first 3 decimals initially
        const shortScore = parseFloat(score).toFixed(3);
        element.textContent = shortScore;
        
        // Show the "Show More" button
        if (showMoreBtn) {
          showMoreBtn.style.display = 'flex';
          
          // Remove any existing event listeners
          const newBtn = showMoreBtn.cloneNode(true);
          showMoreBtn.parentNode.replaceChild(newBtn, showMoreBtn);
          
          // Add new event listener
          newBtn.addEventListener('click', function() {
            const isExpanded = element.getAttribute('data-is-expanded') === 'true';
            
            if (isExpanded) {
              // Show short version
              element.textContent = parseFloat(score).toFixed(3);
              element.setAttribute('data-is-expanded', 'false');
              newBtn.querySelector('.show-more-text').textContent = 'Show More';
              newBtn.classList.remove('expanded');
              
              // Reset any inline styles and classes
              element.style.fontSize = '';
              element.classList.remove('long-number');
            } else {
              // Show full version
              element.textContent = fullScore;
              element.setAttribute('data-is-expanded', 'true');
              newBtn.querySelector('.show-more-text').textContent = 'Show Less';
              newBtn.classList.add('expanded');
              
              // Add class for long numbers
              if (fullScore.length > 15) {
                element.classList.add('long-number');
              }
              
              // Adjust font size dynamically based on length and screen size
              const screenWidth = window.innerWidth;
              const numberLength = fullScore.length;
              
              if (screenWidth <= 480) {
                // Extra small screens (phones in portrait)
                if (numberLength > 20) {
                  element.style.fontSize = '1.2em';
                } else if (numberLength > 15) {
                  element.style.fontSize = '1.4em';
                } else {
                  element.style.fontSize = '1.8em';
                }
              } else if (screenWidth <= 768) {
                // Small screens (phones in landscape, small tablets)
                if (numberLength > 20) {
                  element.style.fontSize = '1.5em';
                } else if (numberLength > 15) {
                  element.style.fontSize = '1.8em';
                } else {
                  element.style.fontSize = '2.2em';
                }
              } else if (screenWidth <= 1024) {
                // Medium screens (tablets)
                if (numberLength > 20) {
                  element.style.fontSize = '2em';
                } else if (numberLength > 15) {
                  element.style.fontSize = '2.5em';
                }
              }
            }
          });
        }
      } else {
        // If 3 or fewer decimals, just show normally
        element.textContent = parseFloat(score).toFixed(2);
        element.removeAttribute('data-full-score');
        element.removeAttribute('data-is-expanded');
        
        // Hide the button
        if (showMoreBtn) {
          showMoreBtn.style.display = 'none';
        }
      }
    }

    // Display breakdown scores with smart formatting (no toggle button for these)
    function displayBreakdownScore(score, element) {
      const fullScore = score.toString();
      const decimalPart = fullScore.split('.')[1] || '';
      
      if (decimalPart.length > 3) {
        // Show first 3 decimals with ellipsis
        const shortScore = parseFloat(score).toFixed(3);
        element.textContent = shortScore + '...';
        element.setAttribute('title', fullScore); // Show full on hover
      } else {
        element.textContent = parseFloat(score).toFixed(2);
        element.removeAttribute('title');
      }
    }

    // Calculate score - Following exact rules
    function calculateScore() {
      const exam1 = parseFloat(exam1Input?.value) || 0;
      const exam2 = parseFloat(exam2Input?.value) || 0;
      const gpa = parseFloat(gpaInput?.value) || 0;

      if (!selectedUniversityType || !selectedExamType || exam1 === 0 || gpa === 0) {
        calculationData.finalScore = 0;
        calculationData.breakdown = {
          exam1Score: 0,
          exam2Score: 0,
          gpaScore: 0
        };
        if (calculatedValue) calculatedValue.textContent = '0.00';
        return;
      }

      let finalScore = 0;
      let exam1Score = 0;
      let exam2Score = 0;
      const uniType = selectedUniversityType;
      const isPublic = uniType === 'government';
      const isPrivate = uniType === 'private' || uniType === 'ahlia';

      // Calculate Exam 1 Score (EST 1, SAT 1, or ACT 1)
      // Handle ACT conversion first (ACT is out of 36, convert to 1600 scale)
      let exam1Normalized = exam1;
      if (selectedExamType === 'ACT') {
        // Convert ACT score (0-36) to EST/SAT scale (0-1600)
        // ACT 36 = 1600, so conversion: (ACT / 36) * 1600
        exam1Normalized = (exam1 / 36) * 1600;
      }
      
      // Rules:
      // Public University: if >= 1090 then score * 69 / 1600, else score * 60 / 1600
      // Private/Ahlia University: if >= 1090 then score * 75 / 1600, else score * 60 / 1600
      if (isPublic) {
        // Public University rules
        if (exam1Normalized >= 1090) {
          exam1Score = exam1Normalized * (69 / 1600);
        } else {
          exam1Score = exam1Normalized * (60 / 1600);
        }
      } else if (isPrivate) {
        // Private or Ahlia University rules
        if (exam1Normalized >= 1090) {
          exam1Score = exam1Normalized * (75 / 1600);
        } else {
          exam1Score = exam1Normalized * (60 / 1600);
        }
      }

      finalScore += exam1Score;

      // Calculate Exam 2 Score (EST 2, ACT 2, or SAT 2)
      // Rule: Score * 15 / 1600 (same for all university types)
      if (selectedExam2Type && selectedExam2Type !== 'NONE' && exam2 > 0) {
        // Handle ACT conversion (ACT is out of 36, convert to 1600 scale)
        let exam2Normalized = exam2;
        if (selectedExam2Type === 'ACT') {
          // Convert ACT score (0-36) to EST/SAT scale (0-1600)
          // ACT 36 = 1600, so conversion: (ACT / 36) * 1600
          exam2Normalized = (exam2 / 36) * 1600;
        }
        exam2Score = exam2Normalized * (15 / 1600);
        finalScore += exam2Score;
      }

      // Calculate GPA Score
      // Rule: GPA * 10 (same for all university types)
      const gpaScore = gpa * 10;
      finalScore += gpaScore;

      // Final Score = Score1 + Score2 + (GPA * 10)
      
      // Save calculation data
      calculationData.exam1 = exam1;
      calculationData.exam2 = exam2;
      calculationData.gpa = gpa;
      calculationData.breakdown.exam1Score = exam1Score;
      calculationData.breakdown.exam2Score = exam2Score;
      calculationData.breakdown.gpaScore = gpaScore;
      calculationData.finalScore = finalScore;

      // Display with smart decimal handling
      if (calculatedValue) {
        displayScoreWithToggle(finalScore, calculatedValue);
      }
    }


    // Calculate and display results
    function calculateAndDisplayResults() {
      if (!selectedUniversityType || !selectedExamType) return;

      const config = universityTypes[selectedUniversityType];
      const examNames = {
        'EST': 'EST',
        'SAT': 'SAT',
        'ACT': 'ACT'
      };
      const examName = examNames[selectedExamType] || 'Exam';

      // Update university type display
      if (selectedUniversityTypeEl) {
        selectedUniversityTypeEl.textContent = config.name;
      }

      // Update calculated value with smart decimal handling
      if (calculatedValue) {
        displayScoreWithToggle(calculationData.finalScore, calculatedValue);
      }

      // Update score icon to be positive (trophy icon)
      const scoreIcon = document.getElementById('scoreIcon');
      if (scoreIcon) {
        if (calculationData.finalScore > 0) {
          scoreIcon.className = 'fas fa-trophy score-icon-positive';
          scoreIcon.style.color = '#B80101';
        } else {
          scoreIcon.className = 'fas fa-calculator score-icon-positive';
          scoreIcon.style.color = '#64748b';
        }
      }

      // Update breakdown labels and values
      if (exam1BreakdownLabel) {
        exam1BreakdownLabel.textContent = `${examName}1 Score:`;
      }
      if (exam1Breakdown) {
        displayBreakdownScore(calculationData.breakdown.exam1Score, exam1Breakdown);
      }
      
      // Show/hide exam 2 breakdown
      if (selectedExam2Type && selectedExam2Type !== 'NONE' && calculationData.exam2 > 0) {
        if (exam2BreakdownItem) exam2BreakdownItem.style.display = 'flex';
        const exam2Name = examNames[selectedExam2Type] || 'Exam';
        if (exam2BreakdownLabel) {
          exam2BreakdownLabel.textContent = `${exam2Name}2 Score:`;
        }
        if (exam2Breakdown) {
          displayBreakdownScore(calculationData.breakdown.exam2Score, exam2Breakdown);
        }
      } else {
        if (exam2BreakdownItem) exam2BreakdownItem.style.display = 'none';
      }
      
      if (gpaBreakdown) {
        displayBreakdownScore(calculationData.breakdown.gpaScore, gpaBreakdown);
      }

      // Display available colleges
      displayAvailableColleges(calculationData.finalScore);
    }

    // Display available colleges based on score
    function displayAvailableColleges(userScore) {
      const availableCollegesCard = document.getElementById('availableCollegesCard');
      const collegesList = document.getElementById('collegesList');
      const collegesCount = document.getElementById('collegesCount');
      const showAllBtn = document.getElementById('showAllCollegesBtn');
      const searchInput = document.getElementById('collegesSearch');

      if (!availableCollegesCard || !collegesList) return;

      // Only show colleges for Government University
      if (selectedUniversityType !== 'government') {
        availableCollegesCard.style.display = 'none';
        return;
      }

      // Filter colleges where user score >= minimum score
      const availableColleges = collegesData.filter(college => userScore >= college.minScore);

      if (availableColleges.length === 0) {
        collegesList.innerHTML = `
          <div class="no-colleges-message">
            <i class="fas fa-graduation-cap"></i>
            <p>Unfortunately, no colleges are available with this score</p>
            <p style="font-size: 0.9em; color: #94a3b8;">Try to improve your score for better options</p>
          </div>
        `;
        availableCollegesCard.style.display = 'block';
        collegesCount.textContent = 'No colleges available';
        showAllBtn.style.display = 'none';
        return;
      }

      // Update count
      collegesCount.textContent = `${availableColleges.length} colleges available based on your score`;

      // Display colleges (show first 10 initially)
      let displayLimit = 10;
      let isExpanded = false;

      function renderColleges(searchTerm = '') {
        let filteredColleges = availableColleges;
        
        if (searchTerm) {
          filteredColleges = availableColleges.filter(college => 
            college.name.includes(searchTerm)
          );
        }

        const collegesToShow = isExpanded ? filteredColleges : filteredColleges.slice(0, displayLimit);

        if (filteredColleges.length === 0) {
          collegesList.innerHTML = `
            <div class="no-colleges-message">
              <i class="fas fa-search"></i>
              <p>No search results found</p>
            </div>
          `;
          showAllBtn.style.display = 'none';
          return;
        }

        collegesList.innerHTML = collegesToShow.map((college, index) => {
          const isHighlighted = index < 5; // Highlight top 5
          return `
            <div class="college-item ${isHighlighted ? 'highlighted' : ''}" data-college="${college.name}">
              <span class="college-name">${college.name}</span>
              <span class="college-score">${college.minScore.toFixed(2)}</span>
            </div>
          `;
        }).join('');

        // Show/hide "Show All" button
        if (filteredColleges.length > displayLimit && !searchTerm) {
          showAllBtn.style.display = 'flex';
          showAllBtn.querySelector('.show-all-text').textContent = isExpanded ? 'Show Less' : `Show All Colleges (${filteredColleges.length})`;
          showAllBtn.classList.toggle('expanded', isExpanded);
        } else {
          showAllBtn.style.display = 'none';
        }
      }

      // Initial render
      renderColleges();

      // Show all button handler
      showAllBtn.onclick = () => {
        isExpanded = !isExpanded;
        renderColleges(searchInput.value);
      };

      // Search handler
      searchInput.oninput = (e) => {
        renderColleges(e.target.value);
      };

      // Show the card
      availableCollegesCard.style.display = 'block';
    }

    // Share results
    function shareResults() {
      if (!selectedUniversityType) return;

      generateAndShareImage();
    }

    // Generate and share results image
    function generateAndShareImage() {
      const canvas = document.createElement('canvas');
      const ctx = canvas.getContext('2d');

      // Set canvas size
      canvas.width = 1000;
      canvas.height = 800;

      // Light mode background gradient (matching the site)
      const gradient = ctx.createLinearGradient(0, 0, canvas.width, canvas.height);
      gradient.addColorStop(0, '#f8fafc');
      gradient.addColorStop(0.3, '#e2e8f0');
      gradient.addColorStop(0.7, '#cbd5e1');
      gradient.addColorStop(1, '#f1f5f9');
      ctx.fillStyle = gradient;
      ctx.fillRect(0, 0, canvas.width, canvas.height);

      // Add subtle grid pattern with enhanced opacity
      ctx.strokeStyle = 'rgba(184, 1, 1, 0.06)';
      ctx.lineWidth = 1;
      for (let i = 0; i < canvas.width; i += 40) {
        ctx.beginPath();
        ctx.moveTo(i, 0);
        ctx.lineTo(i, canvas.height);
        ctx.stroke();
      }
      for (let i = 0; i < canvas.height; i += 40) {
        ctx.beginPath();
        ctx.moveTo(0, i);
        ctx.lineTo(canvas.width, i);
        ctx.stroke();
      }

      // Enhanced Elkably branding - centered and prominent
      const brandX = canvas.width / 2;
      const brandY = 60;

      // Main brand name with gradient effect
      const brandGradient = ctx.createLinearGradient(brandX - 100, brandY - 20, brandX + 100, brandY + 20);
      brandGradient.addColorStop(0, '#B80101');
      brandGradient.addColorStop(0.5, '#8B0000');
      brandGradient.addColorStop(1, '#B80101');
      
      ctx.fillStyle = brandGradient;
      ctx.font = 'bold 42px Arial';
      ctx.textAlign = 'center';
      ctx.textBaseline = 'middle';
      
      // Add text shadow effect
      ctx.shadowColor = 'rgba(184, 1, 1, 0.3)';
      ctx.shadowBlur = 10;
      ctx.shadowOffsetX = 0;
      ctx.shadowOffsetY = 2;
      ctx.fillText('Elkably', brandX, brandY);
      
      // Reset shadow
      ctx.shadowColor = 'transparent';
      ctx.shadowBlur = 0;
      ctx.shadowOffsetX = 0;
      ctx.shadowOffsetY = 0;

      // Subtitle with enhanced styling
      ctx.fillStyle = '#64748b';
      ctx.font = '20px Arial';
      ctx.textAlign = 'center';
      ctx.fillText('University Score Calculator', brandX, brandY + 35);

      // Enhanced main results card background
      const cardGradient = ctx.createLinearGradient(150, 180, canvas.width - 150, 680);
      cardGradient.addColorStop(0, 'rgba(255, 255, 255, 0.95)');
      cardGradient.addColorStop(0.5, 'rgba(248, 250, 252, 0.9)');
      cardGradient.addColorStop(1, 'rgba(255, 255, 255, 0.95)');

      ctx.fillStyle = cardGradient;
      ctx.strokeStyle = 'rgba(184, 1, 1, 0.15)';
      ctx.lineWidth = 3;

      // Draw main card with rounded corners and shadow effect
      ctx.beginPath();
      ctx.roundRect(150, 180, canvas.width - 300, 500, 24);
      ctx.fill();
      
      // Add subtle inner shadow
      ctx.shadowColor = 'rgba(0, 0, 0, 0.1)';
      ctx.shadowBlur = 20;
      ctx.shadowOffsetX = 0;
      ctx.shadowOffsetY = 5;
      ctx.stroke();
      
      // Reset shadow
      ctx.shadowColor = 'transparent';
      ctx.shadowBlur = 0;

      // Enhanced results header section - Better structured layout
      const config = universityTypes[selectedUniversityType];
      const headerX = 200;
      const headerY = 220;

      // Final Score title with enhanced typography (moved to top)
      ctx.fillStyle = '#1e293b';
      ctx.font = 'bold 32px Arial';
      ctx.textAlign = 'left';
      ctx.textBaseline = 'top';
      ctx.fillText('Final Score', headerX, headerY - 10);

      // Medal icon next to title (not overlapping)
      ctx.fillStyle = '#B80101';
      ctx.font = 'bold 36px Arial';
      ctx.textAlign = 'left';
      ctx.textBaseline = 'top';
      
      // Add glow to medal
      ctx.shadowColor = 'rgba(184, 1, 1, 0.3)';
      ctx.shadowBlur = 8;
      const titleWidth = ctx.measureText('Final Score').width;
      ctx.fillText('ðŸ†', headerX + titleWidth + 15, headerY - 5);
      ctx.shadowColor = 'transparent';
      ctx.shadowBlur = 0;

      // University type with better styling (below title)
      ctx.fillStyle = '#64748b';
      ctx.font = '18px Arial';
      ctx.textAlign = 'left';
      ctx.textBaseline = 'top';
      ctx.fillText(config.name, headerX, headerY + 35);

      // Score display section (centered, well-spaced) - Enhanced design
      const centerX = canvas.width / 2;
      const scoreY = 340; // Moved down to prevent overlap

      // Score background circle with gradient (subtle)
      const scoreCircleGradient = ctx.createRadialGradient(centerX, scoreY, 0, centerX, scoreY, 90);
      scoreCircleGradient.addColorStop(0, 'rgba(184, 1, 1, 0.08)');
      scoreCircleGradient.addColorStop(0.5, 'rgba(184, 1, 1, 0.04)');
      scoreCircleGradient.addColorStop(1, 'rgba(184, 1, 1, 0.01)');
      
      ctx.beginPath();
      ctx.arc(centerX, scoreY, 90, 0, 2 * Math.PI);
      ctx.fillStyle = scoreCircleGradient;
      ctx.fill();

      // Score value with enhanced gradient - properly spaced
      const scoreGradient = ctx.createLinearGradient(centerX - 120, scoreY - 50, centerX + 120, scoreY + 50);
      scoreGradient.addColorStop(0, '#B80101');
      scoreGradient.addColorStop(0.5, '#8B0000');
      scoreGradient.addColorStop(1, '#B80101');
      
      ctx.fillStyle = scoreGradient;
      ctx.font = 'bold 96px Arial';
      ctx.textAlign = 'center';
      ctx.textBaseline = 'middle';
      
      // Add glow effect
      ctx.shadowColor = 'rgba(184, 1, 1, 0.4)';
      ctx.shadowBlur = 20;
      ctx.shadowOffsetX = 0;
      ctx.shadowOffsetY = 0;
      
      // Draw score with percentage sign - properly calculated spacing
      const scoreText = calculationData.finalScore.toFixed(2);
      const scoreWidth = ctx.measureText(scoreText).width;
      
      // Draw score number
      ctx.fillText(scoreText, centerX - 25, scoreY);
      
      // Add percentage sign with proper spacing (not overlapping)
      ctx.font = 'bold 54px Arial';
      ctx.fillText('%', centerX + scoreWidth / 2 + 20, scoreY);
      
      // Reset shadow
      ctx.shadowColor = 'transparent';
      ctx.shadowBlur = 0;

      // Enhanced score breakdown section - Better structured and spaced
      const breakdownY = 460; // Moved down to prevent overlap with score

      // Breakdown background with gradient
      const breakdownGradient = ctx.createLinearGradient(180, breakdownY, canvas.width - 180, breakdownY + 240);
      breakdownGradient.addColorStop(0, 'rgba(255, 255, 255, 0.7)');
      breakdownGradient.addColorStop(0.5, 'rgba(248, 250, 252, 0.6)');
      breakdownGradient.addColorStop(1, 'rgba(255, 255, 255, 0.7)');
      
      ctx.fillStyle = breakdownGradient;
      ctx.strokeStyle = 'rgba(184, 1, 1, 0.12)';
      ctx.lineWidth = 2;
      ctx.beginPath();
      ctx.roundRect(180, breakdownY, canvas.width - 360, 240, 18);
      ctx.fill();
      ctx.stroke();

      // Breakdown header
      ctx.fillStyle = '#64748b';
      ctx.font = 'bold 16px Arial';
      ctx.textAlign = 'left';
      ctx.textBaseline = 'top';
      ctx.fillText('Score Breakdown', 200, breakdownY + 20);

      // Breakdown items with proper spacing and styling
      const examNames = {
        'EST': 'EST',
        'SAT': 'SAT',
        'ACT': 'ACT'
      };
      const examName = examNames[selectedExamType] || 'Exam';
      
      const breakdownItems = [{
          label: `${examName}1 Score:`,
          value: calculationData.breakdown.exam1Score.toFixed(2)
        },
        {
          label: 'GPA Score:',
          value: calculationData.breakdown.gpaScore.toFixed(2)
        }
      ];

      // Add Exam 2 if selected
      if (selectedExam2Type && selectedExam2Type !== 'NONE' && calculationData.exam2 > 0) {

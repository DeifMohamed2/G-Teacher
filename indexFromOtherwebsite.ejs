  .results-score .score-value {
    font-size: 3.5em;
    font-weight: bold;
    color: #B80101;
    line-height: 1.1;
    word-break: break-all;
    max-width: 100%;
    overflow-wrap: break-word;
    display: inline-block;
    text-align: center;
    transition: font-size 0.3s ease;

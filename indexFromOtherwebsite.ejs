<script>
  // Brilliant Students Horizontal Slider - Smooth Scrolling
  document.addEventListener('DOMContentLoaded', function() {
    // Initialize slider for each test type
    const sliderConfigs = [
      { 
        type: 'EST', 
        prevId: 'studentsCarouselPrevEST', 
        nextId: 'studentsCarouselNextEST', 
        trackId: 'studentsTrackEST',
        containerId: 'studentsSlidesContainerEST',
        dotsId: 'studentsCarouselIndicatorsEST'
      },
      { 
        type: 'DSAT', 
        prevId: 'studentsCarouselPrevDSAT', 
        nextId: 'studentsCarouselNextDSAT', 
        trackId: 'studentsTrackDSAT',
        containerId: 'studentsSlidesContainerDSAT',
        dotsId: 'studentsCarouselIndicatorsDSAT'
      },
      { 
        type: 'ACT', 
        prevId: 'studentsCarouselPrevACT', 
        nextId: 'studentsCarouselNextACT', 
        trackId: 'studentsTrackACT',
        containerId: 'studentsSlidesContainerACT',
        dotsId: 'studentsCarouselIndicatorsACT'
      }
    ];

    sliderConfigs.forEach(config => {
      const prevBtn = document.getElementById(config.prevId);
      const nextBtn = document.getElementById(config.nextId);
      const track = document.getElementById(config.trackId);
      const container = document.getElementById(config.containerId);
      const dotsContainer = document.getElementById(config.dotsId);
      
      if (!prevBtn || !nextBtn || !track || !container) {
        return;
      }

      const slides = container.querySelectorAll('.student-slide');
      
      if (slides.length === 0) {
        // Hide navigation if no students
        if (prevBtn) prevBtn.style.display = 'none';
        if (nextBtn) nextBtn.style.display = 'none';
        if (dotsContainer) dotsContainer.style.display = 'none';
        return;
      }

      let scrollPosition = 0;
      let isScrolling = false;
      let cardWidth = 0;
      let cardGap = 20;
      let visibleCards = 0;
      let maxScroll = 0;
      let totalPages = 0;
      let dots = [];

      // Calculate dimensions
      function calculateDimensions() {
        if (slides.length === 0) return;
        
        const firstCard = slides[0].querySelector('.student-card-new');
        if (firstCard) {
          const cardRect = firstCard.getBoundingClientRect();
          cardWidth = cardRect.width;
        }
        
        const trackRect = track.getBoundingClientRect();
        const trackWidth = trackRect.width;
        
        // Get track padding (left + right)
        const trackStyles = window.getComputedStyle(track);
        const trackPaddingLeft = parseFloat(trackStyles.paddingLeft) || 0;
        const trackPaddingRight = parseFloat(trackStyles.paddingRight) || 0;
        const trackPadding = trackPaddingLeft + trackPaddingRight;
        
        // Check if mobile view (viewport width <= 768px)
        const isMobile = window.innerWidth <= 768;
        
        // Calculate how many cards fit in viewport
        // On mobile, always show only 1 card
        if (isMobile) {
          visibleCards = 1;
        } else {
          visibleCards = Math.floor(trackWidth / (cardWidth + cardGap));
        }
        
        // Calculate total width of all cards including gaps
        const totalWidth = slides.length * (cardWidth + cardGap) - cardGap;
        
        // Calculate available scrollable width (track width minus padding)
        const availableWidth = trackWidth - trackPadding;
        
        if (isMobile) {
          // Mobile: Allow scrolling until last card can be centered
          // Last card's center position: (slides.length - 1) * (cardWidth + cardGap) + cardWidth / 2
          // To center it, we need: scrollPosition = lastCardCenter - availableWidth / 2
          const lastCardIndex = slides.length - 1;
          const lastCardLeftEdge = lastCardIndex * (cardWidth + cardGap);
          const lastCardCenter = lastCardLeftEdge + cardWidth / 2;
          
          // Max scroll allows last card to be centered
          maxScroll = Math.max(0, lastCardCenter - availableWidth / 2);
          
          // Also ensure we can scroll enough to show the last card fully
          const lastCardRightEdge = lastCardLeftEdge + cardWidth;
          const maxScrollForFullVisibility = Math.max(0, lastCardRightEdge - availableWidth + trackPaddingRight);
          
          // Use the maximum of both to ensure we can both center and fully show the last card
          maxScroll = Math.max(maxScroll, maxScrollForFullVisibility);
        } else {
          // Desktop: Allow scrolling until last card can be centered or at least fully visible with padding
          const lastCardIndex = slides.length - 1;
          const lastCardLeftEdge = lastCardIndex * (cardWidth + cardGap);
          const lastCardCenter = lastCardLeftEdge + cardWidth / 2;
          
          // Option 1: Center the last card
          const scrollToCenter = Math.max(0, lastCardCenter - availableWidth / 2);
          
          // Option 2: Show last card fully with padding
          const lastCardRightEdge = lastCardLeftEdge + cardWidth;
          const scrollToShowFully = Math.max(0, lastCardRightEdge - availableWidth + (availableWidth / 2 - cardWidth / 2));
          
          // Use the maximum to allow both centering and full visibility
          maxScroll = Math.max(scrollToCenter, scrollToShowFully);
          
          // Add extra padding to ensure the last card appears nicely
          maxScroll += (availableWidth / 2 - cardWidth / 2);
        }
        
        // Calculate total pages (number of scroll positions)
        // On mobile, scroll one card at a time. On desktop, scroll by 2-3 cards
        const cardsPerScroll = isMobile ? 1 : Math.max(2, Math.floor(visibleCards * 0.6)); // Scroll 60% of visible cards on desktop
        const scrollAmount = (cardWidth + cardGap) * cardsPerScroll;
        // On mobile, total pages equals number of cards since we show one at a time
        totalPages = isMobile ? slides.length : (maxScroll > 0 ? Math.ceil(maxScroll / scrollAmount) + 1 : 1);
        
        // Update dots
        updateDots();
      }

      // Update scroll position smoothly
      function updateScrollPosition() {
        if (isScrolling) return;
        
        isScrolling = true;
        container.style.transform = `translateX(-${scrollPosition}px)`;
        
        // Update button states
        updateButtonStates();
        
        // Update active dot
        updateActiveDot();
        
        requestAnimationFrame(() => {
          isScrolling = false;
        });
      }
      
      // Create dots dynamically
      function updateDots() {
        if (!dotsContainer) return;
        
        // Clear existing dots
        dotsContainer.innerHTML = '';
        dots = [];
        
        // Only show dots if there's more than one page
        if (totalPages <= 1) {
          dotsContainer.style.display = 'none';
          return;
        }
        
        dotsContainer.style.display = 'flex';
        
        // Create dots for each page
        for (let i = 0; i < totalPages; i++) {
          const dot = document.createElement('button');
          dot.className = 'students-dot';
          if (i === 0) dot.classList.add('active');
          dot.setAttribute('data-page', i);
          dot.setAttribute('aria-label', `Go to page ${i + 1}`);
          
          dot.addEventListener('click', () => {
            scrollToPage(i);
          }, { passive: true });
          
          dotsContainer.appendChild(dot);
          dots.push(dot);
        }
      }
      
      // Update active dot based on scroll position
      function updateActiveDot() {
        if (dots.length === 0 || totalPages <= 1) return;
        
        calculateDimensions();
        const isMobile = window.innerWidth <= 768;
        const cardsPerScroll = isMobile ? 1 : Math.max(2, Math.floor(visibleCards * 0.6));
        const scrollAmount = (cardWidth + cardGap) * cardsPerScroll;
        
        // On mobile, calculate page based on which card is centered
        let currentPage;
        if (isMobile) {
          const trackRect = track.getBoundingClientRect();
          const trackStyles = window.getComputedStyle(track);
          const trackPaddingLeft = parseFloat(trackStyles.paddingLeft) || 0;
          const availableWidth = trackRect.width - trackPaddingLeft - parseFloat(trackStyles.paddingRight || 0);
          const centerX = availableWidth / 2;
          
          // Find which card is closest to center
          let closestIndex = 0;
          let closestDistance = Infinity;
          slides.forEach((slide, index) => {
            const cardLeftEdge = index * (cardWidth + cardGap) - scrollPosition;
            const cardCenter = cardLeftEdge + cardWidth / 2;
            const distance = Math.abs(cardCenter - centerX);
            if (distance < closestDistance) {
              closestDistance = distance;
              closestIndex = index;
            }
          });
          currentPage = closestIndex;
        } else {
          currentPage = Math.round(scrollPosition / scrollAmount);
        }
        
        const activePage = Math.min(currentPage, totalPages - 1);
        
        dots.forEach((dot, index) => {
          dot.classList.toggle('active', index === activePage);
        });
      }
      
      // Scroll to specific page
      function scrollToPage(pageIndex) {
        if (isScrolling || pageIndex < 0 || pageIndex >= totalPages) return;
        
        calculateDimensions();
        const isMobile = window.innerWidth <= 768;
        
        if (isMobile) {
          // Mobile: Center the selected card
          const cardIndex = pageIndex;
          const trackRect = track.getBoundingClientRect();
          const trackStyles = window.getComputedStyle(track);
          const trackPaddingLeft = parseFloat(trackStyles.paddingLeft) || 0;
          const availableWidth = trackRect.width - trackPaddingLeft - parseFloat(trackStyles.paddingRight || 0);
          const centerX = availableWidth / 2;
          
          const cardCenter = cardIndex * (cardWidth + cardGap) + cardWidth / 2;
          scrollPosition = Math.max(0, Math.min(cardCenter - centerX, maxScroll));
        } else {
          const cardsPerScroll = Math.max(2, Math.floor(visibleCards * 0.6));
          const scrollAmount = (cardWidth + cardGap) * cardsPerScroll;
          
          // If it's the last page, scroll to maxScroll to show the last card fully
          if (pageIndex === totalPages - 1) {
            scrollPosition = maxScroll;
          } else {
            scrollPosition = Math.min(pageIndex * scrollAmount, maxScroll);
          }
        }
        
        updateScrollPosition();
      }

      // Update button states (always enabled for looping)
      function updateButtonStates() {
        // Buttons are always enabled for infinite looping
        if (prevBtn) {
          prevBtn.style.opacity = '1';
          prevBtn.style.pointerEvents = 'auto';
        }

        if (nextBtn) {
          nextBtn.style.opacity = '1';
          nextBtn.style.pointerEvents = 'auto';
        }
      }

      // Snap to center for mobile (centers the nearest card)
      function snapToCenter() {
        const isMobile = window.innerWidth <= 768;
        if (!isMobile) return;
        
        calculateDimensions();
        const trackRect = track.getBoundingClientRect();
        const trackStyles = window.getComputedStyle(track);
        const trackPaddingLeft = parseFloat(trackStyles.paddingLeft) || 0;
        const availableWidth = trackRect.width - trackPaddingLeft - parseFloat(trackStyles.paddingRight || 0);
        const centerX = availableWidth / 2;
        
        // Find the card closest to center
        let closestCard = null;
        let closestDistance = Infinity;
        
        slides.forEach((slide, index) => {
          const card = slide.querySelector('.student-card-new');
          if (!card) return;
          
          const cardLeftEdge = index * (cardWidth + cardGap) - scrollPosition;
          const cardCenter = cardLeftEdge + cardWidth / 2;
          const distance = Math.abs(cardCenter - centerX);
          
          if (distance < closestDistance) {
            closestDistance = distance;
            closestCard = { index, cardCenter: index * (cardWidth + cardGap) + cardWidth / 2 };
          }
        });
        
        if (closestCard) {
          // Center this card
          const targetScroll = closestCard.cardCenter - centerX;
          scrollPosition = Math.max(0, Math.min(targetScroll, maxScroll));
          updateScrollPosition();
        }
      }

      // Scroll functions - scroll by multiple cards for bigger movement (with looping)
      function scrollNext() {
        if (isScrolling) return;
        
        calculateDimensions();
        const isMobile = window.innerWidth <= 768;
        const threshold = 10;
        
        // Check if we're at the end (within threshold)
        if (scrollPosition >= maxScroll - threshold) {
          // Loop back to the start
          scrollPosition = 0;
          updateScrollPosition();
          return;
        }
        
        if (isMobile) {
          // Mobile: Scroll to next card and center it
          const currentCardIndex = Math.round(scrollPosition / (cardWidth + cardGap));
          const nextCardIndex = currentCardIndex + 1;
          
          // If we've reached the last card, loop to first
          if (nextCardIndex >= slides.length) {
            scrollPosition = 0;
          } else {
            const trackRect = track.getBoundingClientRect();
            const trackStyles = window.getComputedStyle(track);
            const trackPaddingLeft = parseFloat(trackStyles.paddingLeft) || 0;
            const availableWidth = trackRect.width - trackPaddingLeft - parseFloat(trackStyles.paddingRight || 0);
            const centerX = availableWidth / 2;
            
            const nextCardCenter = nextCardIndex * (cardWidth + cardGap) + cardWidth / 2;
            const targetScroll = nextCardCenter - centerX;
            
            scrollPosition = Math.max(0, Math.min(targetScroll, maxScroll));
          }
        } else {
          // Desktop: Calculate how many cards to scroll (2-3 cards for bigger movement)
          const cardsPerScroll = Math.max(2, Math.floor(visibleCards * 0.6)); // Scroll 60% of visible cards
          const scrollAmount = (cardWidth + cardGap) * cardsPerScroll;
          
          // If we're close to the end, loop to start
          if (scrollPosition + scrollAmount >= maxScroll - threshold) {
            scrollPosition = 0;
          } else {
            scrollPosition = Math.min(scrollPosition + scrollAmount, maxScroll);
          }
        }
        
        updateScrollPosition();
      }

      function scrollPrev() {
        if (isScrolling) return;
        
        calculateDimensions();
        const isMobile = window.innerWidth <= 768;
        const threshold = 10;
        
        // Check if we're at the start (within threshold)
        if (scrollPosition <= threshold) {
          // Loop to the end
          scrollPosition = maxScroll;
          updateScrollPosition();
          return;
        }
        
        if (isMobile) {
          // Mobile: Scroll to previous card and center it
          const currentCardIndex = Math.round(scrollPosition / (cardWidth + cardGap));
          const prevCardIndex = currentCardIndex - 1;
          
          // If we're at the first card, loop to last
          if (prevCardIndex < 0) {
            scrollPosition = maxScroll;
          } else {
            const trackRect = track.getBoundingClientRect();
            const trackStyles = window.getComputedStyle(track);
            const trackPaddingLeft = parseFloat(trackStyles.paddingLeft) || 0;
            const availableWidth = trackRect.width - trackPaddingLeft - parseFloat(trackStyles.paddingRight || 0);
            const centerX = availableWidth / 2;
            
            const prevCardCenter = prevCardIndex * (cardWidth + cardGap) + cardWidth / 2;
            const targetScroll = prevCardCenter - centerX;
            
            scrollPosition = Math.max(0, Math.min(targetScroll, maxScroll));
          }
        } else {
          // Desktop: Calculate how many cards to scroll (2-3 cards for bigger movement)
          const cardsPerScroll = Math.max(2, Math.floor(visibleCards * 0.6)); // Scroll 60% of visible cards
          const scrollAmount = (cardWidth + cardGap) * cardsPerScroll;
          
          // If we're at the start, loop to end
          if (scrollPosition - scrollAmount <= threshold) {
            scrollPosition = maxScroll;
          } else {
            scrollPosition = Math.max(scrollPosition - scrollAmount, 0);
          }
        }
        
        updateScrollPosition();
      }

      // Initialize on load
      function initSlider() {
        // Wait for images to load for accurate dimensions
        setTimeout(() => {
          calculateDimensions();
          updateButtonStates();
          updateActiveDot();
        }, 100);
      }

      // Event listeners
      if (nextBtn) {
        nextBtn.addEventListener('click', scrollNext, { passive: true });
      }

      if (prevBtn) {
        prevBtn.addEventListener('click', scrollPrev, { passive: true });
      }

      // Handle window resize
      let resizeTimer;
      window.addEventListener('resize', () => {
        clearTimeout(resizeTimer);
        resizeTimer = setTimeout(() => {
          calculateDimensions();
          // Clamp scroll position to new max
          scrollPosition = Math.min(scrollPosition, maxScroll);
          updateScrollPosition();
        }, 150);
      }, { passive: true });

      // Touch/swipe support for mobile
      let touchStartX = 0;
      let touchEndX = 0;
      
      container.addEventListener('touchstart', (e) => {
        touchStartX = e.changedTouches[0].screenX;
      }, { passive: true });

      container.addEventListener('touchend', (e) => {
        touchEndX = e.changedTouches[0].screenX;
        handleSwipe();
      }, { passive: true });

      function handleSwipe() {
        const swipeThreshold = 50;
        const diff = touchStartX - touchEndX;
        
        if (Math.abs(diff) > swipeThreshold) {
          if (diff > 0) {
            scrollNext();
          } else {
            scrollPrev();
          }
        } else {
          // Small swipe - snap to nearest center
          snapToCenter();
        }
      }

      // Initialize slider
      initSlider();
      
      // Recalculate after images load
      const images = container.querySelectorAll('img');
      let imagesLoaded = 0;
      images.forEach(img => {
        if (img.complete) {
          imagesLoaded++;
        } else {
          img.addEventListener('load', () => {
            imagesLoaded++;
            if (imagesLoaded === images.length) {
              calculateDimensions();
              updateButtonStates();
              updateActiveDot();
            }
          }, { once: true });
        }
      });
      
      if (imagesLoaded === images.length) {
        calculateDimensions();
        updateButtonStates();
        updateActiveDot();
      }
    });
  });
</script>

